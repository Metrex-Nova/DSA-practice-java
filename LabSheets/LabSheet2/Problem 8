#include<stdio.h>
#include<stdlib.h>
#include<stdbool.h>
#include<string.h>
int main(){
    printf("Hello World!\n");
    int n;
    printf("Please tell the size of first array: ");
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i<n ; i++){
        printf("Please enter the %d element of the first array: ", i+1);
        scanf("%d", &arr[i]);
    }
    int brr[n+2];
    for(int i = 0; i<n+2; i++){
        printf("Please enter the %d element of the second array: ", i+1);
        scanf("%d", &brr[i]);
    }
    for(int i = 0; i<n+2; i++){
        bool flag = false;
        for(int j = 0; j<n; j++){
            if(brr[i] == arr[j]){
                flag = true;
            }
        }
        if(!flag){
            printf("%d ", brr[i]);
        }
    }
    return 0;
}
